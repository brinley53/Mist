<Window x:Class="Mist.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mist"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="HeartrateTextBlock" HorizontalAlignment="Left" Margin="176,106,0,0" TextWrapping="Wrap" Text="{Binding Heartrate.Value}" VerticalAlignment="Top"/>
        <TextBlock x:Name="SoundTextBlock" HorizontalAlignment="Left" Margin="230,106,0,0" TextWrapping="Wrap" Text="50 dB" VerticalAlignment="Top"/>
        <TextBlock x:Name="PeopleTextBlock" HorizontalAlignment="Left" Margin="458,109,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top"/>
        <TextBlock x:Name="SkinResistanceTextBlock" HorizontalAlignment="Left" Margin="106,106,0,0" TextWrapping="Wrap" Text="{Binding SkinResistance.Value}" VerticalAlignment="Top"/>
        <TextBlock x:Name="BodyTemperatureTextBlock" HorizontalAlignment="Left" Margin="325,110,0,0" TextWrapping="Wrap" Text="{Binding BodyTemperature.Value}" VerticalAlignment="Top"/>
        <TextBlock x:Name="Time" HorizontalAlignment="Left" Margin="379,109,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top"/>
        <Label Content="Stress Level" HorizontalAlignment="Left" Margin="300,200,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="StressLevel" Text="{Binding StressLevel}" Margin="325, 225,0,0"/>
        <Label Content="Risk Level" HorizontalAlignment="Left" Margin="500,200,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="RiskLevel" Text="None" Margin="520,225,0,0"/>
        <Label Content="Skin Resistance" HorizontalAlignment="Left" Margin="67,80,0,0" VerticalAlignment="Top"/>
        <Label Content="Heartrate" HorizontalAlignment="Left" Margin="163,80,0,0" VerticalAlignment="Top"/>
        <Label Content="Sound" HorizontalAlignment="Left" Margin="226,80,0,0" VerticalAlignment="Top"/>
        <Label Content="Body Temp" HorizontalAlignment="Left" Margin="300,80,0,0" VerticalAlignment="Top"/>
        <Label Content="Time" HorizontalAlignment="Left" Margin="368,83,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.536,0.541"/>
        <Label Content="People" HorizontalAlignment="Left" Margin="438,83,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
